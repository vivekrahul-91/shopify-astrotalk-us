<script>
(function() {
  'use strict';

  function initWhatsAppClickTracking() {
    try {
      const whatsappButtons = document.querySelectorAll('.whatsapp-box a.btn');

      whatsappButtons.forEach(function(button) {
        if (button.dataset.trackingInitialized) {
          return;
        }
        button.dataset.trackingInitialized = 'true';

        button.addEventListener('click', function(e) {
          // Prevent the link from navigating immediately
          e.preventDefault();

          const destinationUrl = this.href;
          
          // Call the Google Ads conversion function
          // and pass the button's URL to it.
          gtag_report_conversion(destinationUrl);
          
          // You can still fire the Meta event if you need it
          if (typeof fbq !== 'undefined') {
             fbq('trackCustom', 'whatsAppClicked', {
                product_id: '{{ product.id }}',
                type: 'Gemstone'
             });
          }
        });
      });
    } catch (error) {
      console.warn('WhatsApp tracking initialization failed:', error);
    }
  }

  // Initialize the tracking
  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', initWhatsAppClickTracking);
  } else {
    initWhatsAppClickTracking();
  }
})();
</script>

