    <script>
      function updateStars() {
        const starElements = document.querySelectorAll('.jdgm-star');

        starElements.forEach((element) => {
            if (element.hasAttribute('jdgm--on') || element.hasAttribute('jdgm--off') || element.hasAttribute('jdgm--half')) {
                setTimeout(() => {
                    if (element.hasAttribute('jdgm--on')) {
                        element.removeAttribute('jdgm--on');
                        element.classList.add('jdgm--on');
                    }
                    if (element.hasAttribute('jdgm--off')) {
                        element.removeAttribute('jdgm--off');
                        element.classList.add('jdgm--off');
                    }
                    if (element.hasAttribute('jdgm--half')) {
                        element.removeAttribute('jdgm--half');
                        element.classList.add('jdgm--half');
                    }
                }, 1000);
            }
        });
      }
      updateStars();
    </script>

